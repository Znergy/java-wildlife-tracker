<div class="well">
  <h1>Add Ranger to System</h1>
  <form action="/ranger/new" method="post">
    <label for="name">Name</label>
    <input type="text" name="name" maxlength="35">
    <label for="name">Badge Number</label>
    <input type="text" name="badge_number" maxlength="2">
    <label for="name">Phone Number</label>
    <input type="text" name="phone_number" maxlength="10">
    <button type="submit" class="btn btn-primary">Add Ranger</button>
  </form>
</div>
